  file = {/home/joshua/Zotero/storage/9IUKPW3I/Chandler, Garrahan - 2010 - Dynamics on the Way to Forming Glass Bubbles in Space-Time(2).pdf},
  issn = {0066-426X},
  journal = {Annu. Rev. Phys. Chem.},
  keywords = {decoupling,dynamic heterogeneity,excitation lines,facilitation,large-deviation methods,nonequilibrium transitions,review},
